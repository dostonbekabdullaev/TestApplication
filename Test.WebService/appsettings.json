{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "DefaultConnectionString": "Data Source=(LocalDB)\\MSSQLLocalDB;AttachDbFilename=$|DataDirectory|\\DB\\LocalDB.mdf$;Integrated Security=True"
  },
  "Application": {
    "IsRedisEnabled": false,
    "RedisPrimaryEndpoint": "localhost:6379",
    "RedisReaderEndpoint": ""
  },
  "AllowedHosts": "*"
}
